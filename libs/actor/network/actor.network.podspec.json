{
  "name": "actor.network",
  "version": "0.1.0",
  "summary": "=nil; Actor C++ Actor Model Network Module Implementation",
  "description": "=nil; Mtl.Actor provides C++17 actor model network module implementation",
  "homepage": "http://database.nil.foundation/projects/network",
  "license": "Boost Software License",
  "authors": {
    "Mikhail Komarov": "nemo@nil.foundation"
  },
  "platforms": {
    "ios": "5.0",
    "osx": "10.7"
  },
  "source": {
    "git": "https://github.com/nilfoundation/network.git",
    "branch": "master"
  },
  "xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "CLANG_CXX_LIBRARY": "libc++",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/include/\""
  },
  "subspecs": [
    {
      "name": "include",
      "subspecs": [
        {
          "name": "nil",
          "subspecs": [
            {
              "name": "actor",
              "subspecs": [
                {
                  "name": "network",
                  "source_files": [
                    "include/nil/actor/network/*.{hpp}"
                  ],
                  "subspecs": [
                    {
                      "name": "backend",
                      "source_files": [
                        "include/nil/actor/network/backend/*.{hpp}"
                      ]
                    },
                    {
                      "name": "basp",
                      "source_files": [
                        "include/nil/actor/network/basp/*.{hpp}"
                      ]
                    }
                  ]
                },
                {
                  "name": "detail",
                  "source_files": [
                    "include/nil/actor/detail/*.{hpp}"
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "src",
      "subspecs": [
        {
          "name": "basp",
          "source_files": [
            "src/basp/*.{cpp}"
          ]
        },
        {
          "name": "network",
          "source_files": [
            "src/network/*.{cpp}"
          ]
        }
      ],
      "source_files": [
        "src/**/*.{cpp}"
      ]
    }
  ]
}
